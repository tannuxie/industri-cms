{
  "connection": "default",
  "collectionName": "streets",
  "info": {
    "name": "street",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "name": {
      "required": true,
      "type": "string"
    },
    "summary": {
      "type": "text",
      "required": true
    },
    "published": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "huvudbild": {
      "type": "component",
      "repeatable": false,
      "component": "content.huvudbild",
      "required": true
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "content.textbox",
        "content.bildbox",
        "content.karta",
        "content.2col-text",
        "content.2col-text-karta",
        "content.2col-text-img"
      ],
      "required": true
    },
    "tags": {
      "collection": "tag",
      "via": "streets",
      "dominant": true
    },
    "city": {
      "required": true,
      "enum": [
        "sävsjö",
        "vrigstad",
        "stockaryd",
        "rörvik",
        "hultagård",
        "hylletofta"
      ],
      "default": "sävsjö",
      "type": "enumeration"
    }
  }
}
