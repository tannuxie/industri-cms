{
  "connection": "default",
  "collectionName": "streets",
  "info": {
    "name": "street",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "name": {
      "required": true,
      "type": "string"
    },
    "summary": {
      "type": "text",
      "required": true
    },
    "published": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "mainimage": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "content.text-content",
        "content.bildbox",
        "content.2col-text-img",
        "content.map",
        "content.2col-text"
      ],
      "required": true
    },
    "tags": {
      "collection": "tag",
      "via": "streets",
      "dominant": true
    },
	"city": {
      "required": true,
      "enum": [
        "sävsjö",
        "vrigstad",
        "stockaryd",
        "rörvik",
        "hultagård",
        "hylletofta"
      ],
      "default": "sävsjö",
      "type": "enumeration"
    }
  }
}
