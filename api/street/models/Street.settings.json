{
  "connection": "default",
  "collectionName": "streets",
  "info": {
    "name": "street",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "name": {
      "required": true,
      "type": "string"
    },
    "summary": {
      "type": "text",
      "required": true
    },
    "published": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "streetimage": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "layout": {
      "type": "enumeration",
      "enum": [
        "huvudbild_fullbredd",
        "huvudbild_vanster",
        "huvudbild_hoger"
      ],
      "default": "huvudbild_fullbredd",
      "required": true
    },
    "bildfokus": {
      "type": "enumeration",
      "enum": [
        "topp",
        "center",
        "botten"
      ],
      "default": "center",
      "required": true
    },
    "city": {
      "required": true,
      "enum": [
        "savsjo",
        "vrigstad",
        "stockaryd",
        "rorvik",
        "hultagard",
        "hylletofta"
      ],
      "default": "savsjo",
      "type": "enumeration"
    },
    "tags": {
      "collection": "tag",
      "via": "streets",
      "dominant": true
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "content.textbox",
        "content.bildbox",
        "content.karta",
        "content.2col-text",
        "content.2col-text-karta",
        "content.2col-text-img"
      ],
      "required": true
    }
  }
}
