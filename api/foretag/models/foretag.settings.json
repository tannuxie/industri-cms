{
  "connection": "default",
  "collectionName": "foretags",
  "info": {
    "name": "foretag"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "contents": {
      "type": "dynamiczone",
      "components": [
        "content.2col-text-img",
        "content.2col-text",
        "content.bildbox",
        "content.map",
        "content.text-content"
      ]
    },
    "published": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "summary": {
      "type": "text",
      "required": true
    },
    "addresses": {
      "type": "component",
      "repeatable": true,
      "component": "foretag.addressbook"
    },
    "mainimage": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    }
  }
}
