{
  "connection": "default",
  "collectionName": "companies",
  "info": {
    "name": "company",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "name": {
      "required": true,
      "type": "string"
    },
    "summary": {
      "required": true,
      "type": "text"
    },
    "published": {
      "required": true,
      "type": "boolean",
      "default": false
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "content.text-content",
        "content.bildbox",
        "content.2col-text-img",
        "content.map",
        "content.2col-text"
      ],
      "required": true
    },
    "address": {
      "type": "component",
      "repeatable": true,
      "component": "foretag.addressbook",
      "required": true
    },
    "mainimage": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "tags": {
      "collection": "tag",
      "via": "companies",
      "dominant": true
    },
	"type": {
      "required": true,
      "enum": [
        "trä",
        "metall",
        "möbler / träförädling",
        "livsmedel",
        "skor & kläder",
        "plast / gummi",
        "övrigt / diverse"
      ],
	  "default": "övrigt / diverse",
      "type": "enumeration"
    },
    "city": {
      "required": true,
      "enum": [
        "sävsjö",
        "vrigstad",
        "stockaryd",
        "rörvik",
        "hultagård",
        "hylletofta"
      ],
      "default": "sävsjö",
      "type": "enumeration"
    },
	"quality": {
      "default": "1",
      "enum": [
        "1",
        "2",
        "3"
      ],
      "type": "enumeration"
    }
  }
}
