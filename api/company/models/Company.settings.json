{
  "connection": "default",
  "collectionName": "companies",
  "info": {
    "name": "company",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "name": {
      "required": true,
      "type": "string"
    },
    "summary": {
      "required": true,
      "type": "text"
    },
    "published": {
      "required": true,
      "type": "boolean",
      "default": false
    },
    "mainimage": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "layout": {
      "type": "enumeration",
      "enum": [
        "huvudbild_fullbredd",
        "huvudbild_vanster",
        "huvudbild_hoger"
      ],
      "default": "huvudbild_fullbredd",
      "required": true
    },
	"bildfokus": {
      "type": "enumeration",
      "enum": [
        "topp",
        "center",
        "botten"
      ],
      "default": "center",
      "required": true
    },
    "address": {
      "type": "component",
      "repeatable": true,
      "component": "foretag.addressbook",
      "required": true
    },
    "type": {
      "required": true,
      "enum": [
        "tra",
        "metall",
        "moblertraforadling",
        "livsmedel",
        "skorklader",
        "plastgummi",
        "ovrigtdiverse"
      ],
      "default": "ovrigtdiverse",
      "type": "enumeration"
    },
    "city": {
      "required": true,
      "enum": [
        "savsjo",
        "vrigstad",
        "stockaryd",
        "rorvik",
        "hultagard",
        "hylletofta"
      ],
      "default": "savsjo",
      "type": "enumeration"
    },
    "quality": {
      "default": "ett",
      "enum": [
        "ett",
        "tva",
        "tre"
      ],
      "type": "enumeration",
      "required": true
    },
    "tags": {
      "collection": "tag",
      "via": "companies",
      "dominant": true
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "content.textbox",
        "content.bildbox",
        "content.karta",
        "content.2col-text",
        "content.2col-text-karta",
        "content.2col-text-img"
      ],
      "required": true
    }
  }
}
