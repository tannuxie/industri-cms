{
  "connection": "default",
  "collectionName": "articles",
  "info": {
    "name": "article",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "title": {
      "required": true,
      "type": "string"
    },
    "published": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "date": {
      "type": "date"
    },
    "articleimage": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "layout": {
      "type": "enumeration",
      "enum": [
        "huvudbild_fullbredd",
        "huvudbild_vanster",
        "huvudbild_hoger"
      ],
      "default": "huvudbild_fullbredd",
      "required": true
    },
    "bildfokus": {
      "type": "enumeration",
      "enum": [
        "topp",
        "center",
        "botten"
      ],
      "default": "center",
      "required": true
    },
    "tags": {
      "collection": "tag",
      "via": "articles",
      "dominant": true
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "content.textbox",
        "content.bildbox",
        "content.karta",
        "content.2col-text",
        "content.2col-text-karta",
        "content.2col-text-img"
      ],
      "required": true
    }
  }
}
